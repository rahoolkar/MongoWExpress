<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Whatsapp</title>
    <style>
        div{
            border: 2px solid black;
            border-radius: 20px;
        }
        .box{
            margin: 20px;
            padding: 20px;
        }
        .box{
            display: inline-block;
        }
    </style>
</head>
<body>
    <h1>Chats</h1>
    <div class="container">
        <% for(let chat of chats){ %>
            <div class="box">
                <p>From:<%= chat.from %></p>
                <div class="msg"><p>Messege: <%= chat.messege %></p></div>
                <p>To: <%= chat.to %></p>
            </div>
        <% } %>
    </div>
</body>
</html>

